function commentFrameH(){var _if = document.getElementById('js_comment_frame');if(null == _if)return;try{if('complete' == _if.contentWindow.document.readyState)_if.height = _if.contentWindow.document.documentElement.scrollHeight;}catch (ex){};};window.setInterval('commentFrameH()', 200); document.write('<iframe id="js_comment_frame" src="http://www.jxvdy.com/comment/index?oid=107&cid=13" width="100%" scrolling="no" frameborder="0"></iframe>');